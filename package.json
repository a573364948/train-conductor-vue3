{
  "name": "train-conductor-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" vite",
    "dev:debug": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192 --inspect\" vite",
    "build": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vue-tsc && vite build",
    "build:skip-types": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "build:analyze": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vue-tsc && vite build --mode analyze",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "cleanup": "rm -rf node_modules/.vite && rm -rf dist"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@supabase/supabase-js": "^2.50.0",
