    "@types/pako": "^2.0.3",
    "@vercel/analytics": "^1.5.0",
    "@vueuse/core": "^10.9.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "element-plus": "^2.6.2",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lodash-es": "^4.17.21",
    "openai": "^5.5.1",
    "pako": "^2.1.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "cross-env": "^7.0.3",
    "sass": "^1.72.0",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.0",
    "vite-plugin-pwa": "^1.0.0",
    "vue-tsc": "^2.0.6"
